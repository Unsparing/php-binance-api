{
    "name": "Unsparing/php-binance-api",
    "description": "PHP Binance API is an asynchronous PHP library for the Binance API designed to be easy to use.",
    "keywords": ["Binance","cryptocurrency","WebSocket"],
    "license": "MIT",
    "repositories": [ {
            "type": "vcs",
            "url": "https://github.com/Unsparing/php-binance-api"
    }],
    "require": {
        "php": ">=5.4",
        "ratchet/pawl": "^0.3.0",
        "react/socket": "^1.0 || ^0.8 || ^0.7",
        "ratchet/rfc6455": "^0.2.3",
        "Unsparing/php-binance-api": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "config": {
    	"optimize-autoloader": true,
        "minimum-stability": "dev"
    },
    "autoload": {
        "classmap": [
          "php-binance-api.php"
        ]
    }
}